{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
		"^common-(.*)": "@/components/common-$1/common-$1.vue"
	},
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/about/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"tabBar": {
		"color": "#969799",
		"height": "117px",
		"fontSize": "26px",
		"selectedColor": "#17A1FF",
		"borderStyle": "black",
		"spacing": "10px",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "首页",
				"iconPath": "/static/images/tabbar/manage.png",
				"selectedIconPath": "/static/images/tabbar/manage_select.png"
			},
			{
				"pagePath": "pages/about/index",
				"text": "关于",
				"iconPath": "/static/images/tabbar/message.png",
				"selectedIconPath": "/static/images/tabbar/message_select.png"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
